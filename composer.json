{
    "name": "yii-tools/tailwind-asset",
    "type": "library",
    "description": "Asset for Tailwind for YiiFramework v.3.0.",
    "keywords": [
        "yii-tools",
        "asset",
        "tailwind",
        "yii3"
    ],
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "foxy/foxy": "^1.2",
        "yiisoft/assets": "^4.0",
        "yiisoft/files": "^2.0"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^4.3",
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.28",
        "vimeo/psalm": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Tailwind\\Asset\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Tailwind\\Asset\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "foxy/foxy": true,
            "infection/extension-installer": true
        }
    }
}
